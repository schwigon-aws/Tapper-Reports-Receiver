#! /usr/bin/env perl

use strict;
use warnings;

use Artemis::Config;
use Artemis::Reports::Receiver;
use Log::Log4perl;

my $port = Artemis::Config->subconfig->{report_port};
my $logconf = Artemis::Config->subconfig->{files}{log4perl_cfg};
Log::Log4perl->init($logconf);

my $receiver = Artemis::Reports::Receiver->new();
$receiver->run($port);
